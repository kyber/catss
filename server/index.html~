<script type="text/javascript"> 
	$(document).ready(function() {
		$(".topMenuAction").click( function() {
			if ($("#openCloseIdentifier").is(":hidden")) {
				$("#slider").animate({ 
					marginTop: "-135px"
					}, 500 );
				$("#topMenuImage").html('<img src="images/open.png" alt="open" />');
				$("#openCloseIdentifier").show();
			} else {
				$("#slider").animate({ 
					marginTop: "0px"
					}, 500 );
				$("#topMenuImage").html('<img src="images/close.png" alt="close" />');
				$("#openCloseIdentifier").hide();
			}
		});  
	});
	</script> 

<script type="text/javascript">
  $(function(){
  
  // Tabs
  $('#tabs').tabs();
 // alert('arrow next');
  $('#c-arrow').rotateRight(0);
  $('a[rel*=facebox]').facebox(); 
  });
</script>
<script type="text/javascript">
  $(document).ready(function() {
    $("#rotate-do").click( function () {
	kulma = document.getElementById('aste').value;
        vanhakulma = document.getElementById('v-aste');
       // alert('reset arrow');
        asteint = parseInt(kulma);
        $('#c-arrow').rotateLeft(parseInt(vanhakulma.value));
       // alert('set new ankle');
        $('#c-arrow').rotateRight(asteint);
        vanhakulma.value = asteint;

    });
});
</script>



